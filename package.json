{
  "name": "protosite-palette",
  "version": "2.14.22",
  "type": "commonjs",
  "types": "./index.d.ts",
  "main": "./index.cjs",
  "module": "./index.mjs",
  "files": [
    "./assets/*",
    "./builder/*",
    "./lib/*"
  ],
  "exports": {
    "import": "./index.mjs",
    "require": "./index.cjs"
  },
  "scripts": {
    "build:all": "node ./builder/build.cjs --dist=temp",
    "build:protosite": "node ./builder/build.cjs --name=protosite --dist=assets",
    "build:material": "node ./builder/build.cjs --name=material --dist=temp",
    "build:universal": "node ./builder/build.cjs --name=universal --dist=temp",
    "fix": "run-p --print-label \"lint:** --cache --color --debug --fix\"",
    "lint:cjs": "set ESLINT_USE_FLAT_CONFIG=true&& eslint . -c .config/eslint.config.cjs",
    "lint:mjs": "set ESLINT_USE_FLAT_CONFIG=true&& eslint . -c .config/eslint.config.mjs",
    "lint:others": "set ESLINT_USE_FLAT_CONFIG=false&& eslint . --ext .json,.yaml"
  },
  "dependencies": {
    "adobe-aco": "0.1.1",
    "bezier": "0.1.0",
    "bezier-easing": "2.1.0",
    "d3-color": "3.1.0",
    "d3-color-difference": "0.1.3",
    "d3-scale": "4.0.2",
    "eslint-plugin-json-format": "2.0.1",
    "hex2rgb": "2.2.0",
    "lodash": "4.17.21"
  },
  "devDependencies": {
    "@stylistic/eslint-plugin": "1.6.2",
    "@typescript-eslint/parser": "7.0.2",
    "eslint": "8.57.0",
    "eslint-plugin-dtslint": "3.0.2",
    "eslint-plugin-json": "3.1.0",
    "eslint-plugin-json-schema-validator": "4.8.3",
    "eslint-plugin-require-sort": "1.3.0",
    "eslint-plugin-sonarjs": "0.24.0",
    "eslint-plugin-yaml": "0.5.0",
    "eslint-plugin-yml": "1.12.2",
    "esm": "3.2.25",
    "esprima": "4.0.1",
    "file-saver": "2.0.5",
    "jsonc-eslint-parser": "2.4.0",
    "node-color-log": "11.0.2",
    "npm-run-all": "4.1.5",
    "read-directory": "3.0.2",
    "type-fest": "4.10.3",
    "typescript": "5.3.3",
    "yaml-eslint-parser": "1.2.2",
    "yargs": "17.7.2"
  },
  "engines": {
    "node": ">=21.1.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BPanchenko/protosite-palette.git"
  },
  "author": {
    "name": "Boris Panchenko",
    "email": "boris.v.panchenko@mail.ru",
    "url": "https://bpanchenko.ru"
  },
  "bugs": {
    "url": "https://github.com/BPanchenko/protosite-palette/issues"
  },
  "homepage": "https://github.com/BPanchenko/protosite-palette#readme",
  "license": "LGPL-3.0-only"
}
