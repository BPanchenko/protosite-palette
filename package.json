{
  "name": "protosite-palette",
  "version": "2.13.22",
  "type": "module",
  "main": "module/index.mjs",
  "types": "module/index.d.ts",
  "files": [
    "assets/*",
    "module/*"
  ],
  "scripts": {
    "build:all": "node builder/build.cjs --dist=temp",
    "build:protosite": "node builder/build.cjs --name=protosite --dist=assets",
    "build:material": "node builder/build.cjs --name=material",
    "build:universal": "node builder/build.cjs --name=universal",
    "fix": "run-p --print-label lint:**",
    "lint:cjs": "set ESLINT_USE_FLAT_CONFIG=true&& eslint --fix -c eslint.config.cjs builder",
    "lint:mjs": "set ESLINT_USE_FLAT_CONFIG=true&& eslint --fix -c eslint.config.mjs module"
  },
  "devDependencies": {
    "adobe-aco": "0.1.1",
    "bezier": "0.1.0",
    "bezier-easing": "2.1.0",
    "d3-color": "3.1.0",
    "d3-color-difference": "0.1.3",
    "d3-scale": "4.0.2",
    "eslint": "8.56.0",
    "eslint-plugin-require-sort": "1.3.0",
    "node-color-log": "11.0.0",
    "npm-run-all": "4.1.5",
    "read-directory": "3.0.2",
    "type-fest": "^4.10.2",
    "yargs": "17.7.2"
  },
  "dependencies": {
    "lodash": "4.17.21"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BPanchenko/protosite-palette.git"
  },
  "author": {
    "name": "Boris Panchenko",
    "email": "boris.v.panchenko@mail.ru",
    "url": "https://bpanchenko.ru"
  },
  "bugs": {
    "url": "https://github.com/BPanchenko/protosite-palette/issues"
  },
  "homepage": "https://github.com/BPanchenko/protosite-palette#readme"
}
